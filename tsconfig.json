/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "paths": {
      "@explorer-panel": [
        "./projects/explorer-panel/src/public-api"
      ],
      "@core": [
        "./projects/core/src/public-api"
      ],
      "@ui-kit": [
        "./projects/ui-kit/src/public-api"
      ],
      "@directory": [
        "./projects/plugins/directory/src/public-api"
      ],
      "@resources": [
        "./projects/resources/src/public-api"
      ],
      "@palette": [
        "./projects/plugins/palette/src/public-api"
      ],
      "@router-controller": [
        "./projects/router-controller/src/public-api"
      ],
      "@foblex/core": [
        "./projects/f-core/src/public-api"
      ],
      "@foblex/flow": [
        "./projects/f-flow/src/public-api"
      ],
      "f-geometry": [
        "./dist/f-geometry"
      ]
    },
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": [
      "ES2022",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
